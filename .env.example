# n0ir Protocol Deployment Configuration
# Copy this file to .env and fill in your values

# RPC URLs
BASE_RPC_URL=https://mainnet.base.org
# Alternative RPC endpoints:
# BASE_RPC_URL=https://base.llamarpc.com
# BASE_RPC_URL=https://base.meowrpc.com

# Private key for deployment (without 0x prefix)
PRIVATE_KEY=

# Contract owner address (defaults to deployer if not set)
OWNER_ADDRESS=

# CDP Wallet addresses to register (comma-separated)
# Example: CDP_WALLETS=0x123...,0x456...,0x789...
CDP_WALLETS=

# Etherscan/Basescan API key for contract verification
BASESCAN_API_KEY=

# Optional: Skip contract verification
SKIP_VERIFICATION=false

# Optional: Custom verifier URL (defaults to Basescan)
VERIFIER_URL=https://api.basescan.org/api
